head
meta(charset='UTF-8')
meta(http-equiv='X-UA-Compatible')
meta(
  name='viewport',
  content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0'
)
meta(name='renderer', content='webkit')
meta(name='color-scheme', content='dark light')
meta(name='author', content=config.author)
meta(name='description', content=config.description)
meta(
  name='keywords',
  content=page.keywords ? page.keywords : config.keywords ? config.keywords : ''
)
title #{ config.title }
link(rel='stylesheet', href='//at.alicdn.com/t/font_3242539_utq6ykfq7z.css')
link(rel='stylesheet', light-src=theme.highlight.light, dark-src=theme.highlight.dark)
link(rel='stylesheet', href='https://fonts.font.im/css?family=Noto+Serif', as='font')
link(href='/style/index.css', rel='stylesheet')
